class Solution {
public:
    string reversePrefix(string word, char ch) {
    
        
        for(int i=0;i<word.size();i++)
        {
            char chh=word[i];
           
            if(ch==chh )
            {
                reverse(word.begin(),word.begin()+i+1);
                break;
            }
            
        }
        return word;
    }
};
